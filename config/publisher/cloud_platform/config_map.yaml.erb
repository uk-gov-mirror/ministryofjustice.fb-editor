apiVersion: v1
kind: ConfigMap
metadata:
  name: <%= config_map_name %>
  namespace: <%= namespace %>
data: <% config_map.each do |configuration|  %>
  <% if configuration.name == 'ENCODED_PUBLIC_KEY' %>
    <%= configuration.name %>: "<%= configuration.encode64 %>"
  <% else %>
    <%= configuration.name %>: "<%= configuration.decrypt_value %>"
  <% end %>
<% end %>
